{
	"name": "@catniplabs/mango",
	"version": "0.4.0",
	"description": "🍊 Fake data generator with a juicy twist — fast, realistic, tiny, TS-first.",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"jsdelivr": "dist/index.mjs",
	"unpkg": "dist/index.mjs",
	"types": "./dist/index.d.ts",
	"keywords": [
		"mango",
		"fake",
		"data",
		"generator",
		"typescript",
		"catniplabs"
	],
	"author": "Anderson Gonçalves",
	"repository": {
		"type": "git",
		"url": "https://github.com/CatnipLabs/Mango"
	},
	"homepage": "https://www.npmjs.com/package/@catniplabs/mango",
	"publishConfig": {
		"access": "public",
		"provenance": true,
		"registry": "https://registry.npmjs.org"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"default": "./dist/index.mjs"
		}
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format": "biome format --write .",
		"changeset": "changeset",
		"prepublishOnly": "pnpm build && pnpm test && pnpm typecheck",
		"prepare": "husky"
	},
	"packageManager": "pnpm@10.14.0",
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@changesets/cli": "^2.29.5",
		"@types/node": "^24.2.1",
		"@vitest/coverage-v8": "^3.2.4",
		"husky": "^9.1.7",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	}
}
